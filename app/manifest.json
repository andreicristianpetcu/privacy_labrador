{
  "name": "__MSG_appName__",
  "short_name": "__MSG_appShortName__",
  "description": "__MSG_appDescription__",
  "version": "0.3.0",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "content_scripts": [{
    "matches": [
      "https://duckduckgo.com/*",
      "https://www.qwant.com/*"
    ],
    "js": [
      "scripts/contentscript.js"
    ],
    "run_at": "document_idle",
    "all_frames": false
  }],
  "permissions": [
    "activeTab"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Google by Privacy Labrador",
      "search_url": "https://www.google.com/search?&q={searchTerms}",
      "keyword": "g",
      "favicon_url": "https://raw.githubusercontent.com/andreicristianpetcu/privacy_labrador/master/app/images/icon-16.png",
      "is_default": false
    }
  },
  "commands": {
    "to-google": {
      "suggested_key": {
        "default": "Alt+Shift+G"
      },
      "description": "Redirects current search results page to Google"
    }
  }
}